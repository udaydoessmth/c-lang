#include <stdio.h>

int main()
{
    int a,b,c;
    printf("enter three numbers: ");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b){ printf("%d is greater than %d",a,b); }
    else{
        if(b>c){ printf("%d is greatest out of given three numbers!!",b); }
        else{ printf("%d is greatest out of given three numbers!!",c); }
    }
    return 0;
}
